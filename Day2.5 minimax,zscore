age=c(23,23,27,27,39,41,47,49,50,52,54,54,56,57,58,58,60,61)
fact=c(9.5,26.5,7.8,17.8,31.4,25.9,27.4,27.2,31.2,34.6,42.5,28.8,33.4,30.2,34.1,32.9,41.2,35.7)
min_age<-min(age)
max_age<-max(age)
norm_age_minmax<-(39-min_age)/(max_age-min_age)
norm_age_minmax

mean_age=mean(age)
mean_age
sd_age=sd(age)
sd_age
norm_age_zscore<-(39-mean_age)/sd_age
norm_age_zscore


max_digits<-max(nchar(as.character(age)))norm_age_decimal<-35/10^max_digits
